#!/Users/zebedeenicholls/Documents/AGCEC/Papers/CMIP6GHG-concentration-projections/CMIP6-json-data-citation-generator/venv2/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'CMIP6-json-data-citation-generator','console_scripts','upload_CMIP6_json_files'
__requires__ = 'CMIP6-json-data-citation-generator'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('CMIP6-json-data-citation-generator', 'console_scripts', 'upload_CMIP6_json_files')()
    )
