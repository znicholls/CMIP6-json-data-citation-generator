#!/Users/zebedeenicholls/Documents/AGCEC/Papers/CMIP6GHG-concentration-projections/CMIP6-json-data-citation-generator/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'codecov','console_scripts','codecov'
__requires__ = 'codecov'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('codecov', 'console_scripts', 'codecov')()
    )
